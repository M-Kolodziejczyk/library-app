}, [isSubmitting]);\n\n  return (\n    <Link to={{ pathname: `/book/${book.id}`, book: book }} className=\"book\">\n      <div className=\"book__container\">\n        <div className=\"book__containerImg\">\n          <div className=\"book__img\">\n            <img src={img} alt=\"\" />\n          </div>\n        </div>\n        <div className=\"book__containerContent\">\n          <div className=\"book__Content\">\n            <p className=\"book__ContentTitle\">{book.title}</p>\n            <p onClick={handleClick} className=\"book__ContentAuthor\">\n              {book.authorName}\n            </p>\n            <p className=\"book__ContentCategory\">{book.category}</p>\n            <p className=\"book__ContentTotalPages\">\n              Total pages: {book.totalPages}\n              {error && (\n                <span className=\"book__ContentBasketError\">\n                  Already added to basket!\n                </span>\n              )}\n            </p>\n          </div>\n          <div className=\"book__Info\">\n            <span className=\"book__InfoStatus\">Available</span>\n\n            <button className=\"book__InfoCartBtn\" onClick={handleBasket}>\n              Add to cart\n            </button>\n          </div>\n        </div>\n      </div>\n    </Link>\n  );\n};\n\nexport default Book;\n",["133","134"],"/home/mk/Pulpit/DEV/library-app/src/pages/AuthorsPage/AuthorsPage.js",[],"/home/mk/Pulpit/DEV/library-app/src/pages/AuthorPage/AuthorPage.js",[],"/home/mk/Pulpit/DEV/library-app/src/pages/BookPage/BookPage.js",["135","136"],"/home/mk/Pulpit/DEV/library-app/src/pages/BasketPage/components/BasketPageBook.js",[],"/home/mk/Pulpit/DEV/library-app/src/pages/UserPage/UserPage.js",["137"],"/home/mk/Pulpit/DEV/library-app/src/pages/UserPage/componets/MyDetails.js",[],"/home/mk/Pulpit/DEV/library-app/src/pages/UserPage/componets/MyBooks.js",[],["138","139"],"/home/mk/Pulpit/DEV/library-app/src/pages/UserPage/componets/AddBook.js",[],"/home/mk/Pulpit/DEV/lib